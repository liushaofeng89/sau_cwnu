<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.cwnu.sau.model.DownloadModel" table="sau_download">
        
        <id name="id" column="file_id" type="java.lang.Integer">
            <generator class="identity" />
        </id>
        
        <property name="fileType" column="file_type" type="java.lang.String" />
        <property name="fileName" column="file_name" type="java.lang.String" />
        <property name="fileExtension" column="file_extension" type="java.lang.String" />
        <property name="fileOwner" column="file_owner" type="java.lang.String" />
        <property name="fileUploadTime" column="file_time" type="java.sql.Timestamp" />
        <property name="fileDownloadCount" column="file_download_count" type="java.lang.Integer" />
    </class>
</hibernate-mapping>